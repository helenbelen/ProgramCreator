<Window x:Name="ProgramCreatorMain" x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="243*"/>
            <ColumnDefinition Width="275*"/>
        </Grid.ColumnDefinitions>
        <ListBox Grid.Column="1" HorizontalAlignment="Left" Height="94" Margin="34.8,28,0,0" VerticalAlignment="Top" Width="177" Background="#FFA4B1D7"/>
        <Button x:Name="RemoveButton" Content="Remove Program" Grid.Column="1" HorizontalAlignment="Left" Margin="9.8,245,0,0" VerticalAlignment="Top" Width="110" RenderTransformOrigin="1.168,0.205"/>
        <Button x:Name="RunButton" Content="Run Program" Grid.Column="1" HorizontalAlignment="Left" Margin="164.8,245,0,0" VerticalAlignment="Top" Width="75" Click="RunButton_Click"/>
        <TextBox x:Name="Program_Output" Grid.Column="1" HorizontalAlignment="Left" Height="52" Margin="34.8,143,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="177" Background="#FFE5D4D4"/>

    </Grid>
</Window>
